<body>
    <!-- All of this should be in a GOOGLE BODY-TOP file -->
    <!-- Google Tag Manager (noscript) -->
    <noscript>
        <iframe src="https://www.googletagmanager.com/ns.html?id=GTM-MW667S3"
                height="0" width="0" style="display:none;visibility:hidden"></iframe>
    </noscript>
    <!-- End Google Tag Manager (noscript) -->
    <!-- Global site tag (gtag.js) - Google Analytics -->
    <script async src="https://www.googletagmanager.com/gtag/js?id=UA-176693214-1"></script>
    <script>window.dataLayer = window.dataLayer || [];
        function gtag() { dataLayer.push(arguments); }
        gtag('js', new Date());

        gtag('config', 'UA-176693214-1');</script>

    <!-- Load `web-vitals` using a module script. -->
    <script type="module">import { getCLS, getFID, getLCP } from 'https://unpkg.com/web-vitals@0.2.4/dist/web-vitals.es5.min.js?module';

        getCLS(console.log);
        getFID(console.log);
        getLCP(console.log);</script>

    <!-- Google Optimize Snippet -->
    <script async src="https://www.googleoptimize.com/optimize.js?id=OPT-5VMLW66"></script>

    <script>/**
                        * Function that registers a click on an outbound link in Analytics.
                        * This function takes a valid URL string as an argument, and uses that URL string
                        * as the event label. Setting the transport method to 'beacon' lets the hit be sent
                        * using 'navigator.sendBeacon' in browser that support it.
                        */
        var getOutboundLink = function (url) {
            gtag('event', 'click', {
                'event_category': 'outbound',
                'event_label': url,
                'transport_type': 'beacon',
                //'event_callback': function(){document.location = url;} - Commented out as we do not want the event to update the current page
            });
        }</script>

    <!-- Google AdSense -->
    <script data-ad-client="ca-pub-8497649303349192" async src="https://pagead2.googlesyndication.com/pagead/js/adsbygoogle.js"></script>
    <!-- END OF GOOGLE BODY-TOP -->
    <!-- Header -->
    <header>
        <!-- Banner -->
        <div class="header-placeholder">
            <div class="header-img">
                &nbsp;
            </div>
            <div class="header-text">
                &nbsp;
            </div>
        </div>
    </header>

    <!-- Main -->
    <main>
        @RenderBody()
    </main>

    <!-- Footer -->
    <footer>
        @* Icons *@
        @{
            var SocialMediaCompanies = new List<SocialMediaCompany>
        {
            new SocialMediaCompany {Name = "Twitch", HomeURL = "https://www.twitch.tv/nicetranslady", Prefix = "twitch" },
            new SocialMediaCompany {Name = "Discord", HomeURL = "https://discord.gg/6XgZYdv", Prefix = "discord" },
            new SocialMediaCompany {Name = "Twitter", HomeURL = "https://twitter.com/nicetranslady", Prefix = "twitter" },
            new SocialMediaCompany {Name = "Instagram", HomeURL = "https://instagram.com/nicetranslady", Prefix = "instagram" },
            new SocialMediaCompany {Name = "Facebook", HomeURL = "https://facebook.com/nicetranslady", Prefix = "facebook" },
            new SocialMediaCompany {Name = "Steam", HomeURL = "https://steamcommunity.com/profiles/76561199034457152", Prefix = "steam" },
            new SocialMediaCompany {Name = "YouTube", HomeURL = "https://www.youtube.com/channel/UCt1TRJ8INljw-8ouamS0fwg", Prefix = "youtube" },
            new SocialMediaCompany {Name = "Patreon", HomeURL = "https://www.patreon.com/nicetranslady", Prefix = "patreon" },
            new SocialMediaCompany {Name = "Blogger", HomeURL = "https://blog.nicetranslady.online", Prefix = "blogger" },
            new SocialMediaCompany {Name = "Amazon", HomeURL = "https://www.amazon.co.uk/hz/wishlist/ls/21B3NNZDQ9TK2?ref_=wl_share", Prefix = "amazon" },
            new SocialMediaCompany {Name = "OnlyFans", HomeURL = "https://onlyfans.com/nicetranslady", Prefix = "onlyfans" },
            new SocialMediaCompany {Name = "PayPal", HomeURL = "https://paypal.me/nicetranslady", Prefix = "paypal" },
            new SocialMediaCompany {Name = "CuriousCat", HomeURL = "https://curiouscat.me/nicetranslady", Prefix = "curiouscat" },
            new SocialMediaCompany {Name = "GitHub", HomeURL = "https://github.com/nicetranslady", Prefix = "github" }
        };
        }

        <div class="container" style="text-align: center">
            <div class="socialMedia-icons">
                @foreach (var SocialMediaCompany in SocialMediaCompanies)
                {
                    <div>
                        <button onclick="window.open('@SocialMediaCompany.HomeURL', '_blank')">
                            <img class="button-icon"
                                 src="/images/social_media/160_160/@(SocialMediaCompany.Prefix)_black.png"
                                 alt="@SocialMediaCompany.Name"
                                 onMouseOver="this.src='/images/social_media/160_160/@(SocialMediaCompany.Prefix)_white.png';"
                                 onMouseOut="this.src='/images/social_media/160_160/@(SocialMediaCompany.Prefix)_black.png';"
                                 onclick="getOutboundLink('@SocialMediaCompany.HomeURL'); return false;" />
                        </button>
                    </div>
                }
            </div>
        </div>

        <div>&nbsp;</div>

        <!-- Copyright and Privacy -->
        <div class="border-top text-muted">
            <div class="container">
                <div style="text-align: center">
                    &copy;&nbsp;@System.DateTime.Now.Year&nbsp;-&nbsp;nicetranslady<!-- | <a asp-area="" asp-page="/Privacy">Privacy</a> -->
                </div>
            </div>
        </div>
    </footer>

    <!-- Load Scripts -->
    <partial name="_ValidationScriptsPartial" />

    @*@RenderSection("Scripts", required: false)*@
</body>
</html>
